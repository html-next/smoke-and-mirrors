{
  "name": "smoke-and-mirrors",
  "version": "0.4.7",
  "description": "Ambitious infinite-scroll and rendering for ambitious applications.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "start": "ember server",
    "build": "ember build",
    "suave": "jscs addon app tests --fix",
